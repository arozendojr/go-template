{
    "configurations": [
        {
            "name": "Local debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/internal/cmd/main.go",
            "envFile": "${workspaceFolder}/.env.sh.default"
        },
        {
            "name": "Remote debug",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 2345,
            "host": "127.0.0.1"
        },
    ]
}
